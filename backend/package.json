{
  "name": "coolestproject-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node --experimental-vm-modules ./node_modules/.bin/jest --config ./test/jest-e2e.json",
    "test:debug_e2e": "node ./node_modules/.bin/jest --runInBand --detectOpenHandles --config ./test/jest-e2e.json --verbose"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.0",
    "@adminjs/nestjs": "^6.1.0",
    "@adminjs/sequelize": "^4.1.1",
    "@azure/storage-blob": "^12.25.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/sequelize": "^10.0.1",
    "@nestjs/swagger": "^7.4.2",
    "adminjs": "^7.6.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.3",
    "nestjs-command": "^3.1.4",
    "nodemailer": "^7.0.3",
    "passport-cookie": "^1.0.9",
    "passport-jwt": "^4.0.1",
    "passport-jwt-cookiecombo": "^1.0.5",
    "passport-saml": "^3.2.4",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.4",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "sequelize-cli": "^6.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 150000
  }
}
